<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.olaoluwaolejede.test.RestaurantsPageActivity"
    android:visibility="visible">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Restaurants"
        android:id="@+id/textView17"
        android:textSize="30dp"
        android:layout_below="@+id/textView34"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="41dp"
        android:textColor="#000000"
        android:textStyle="bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ABC Kitchen"
        android:id="@+id/TextViewC"
        android:textSize="30dp"
        android:textAlignment="center"
        android:layout_marginTop="38dp"
        android:layout_below="@+id/textView17"
        android:layout_centerHorizontal="true"
        android:textColor="#ffa500" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Budget: $20"
        android:id="@+id/textView34"
        android:textSize="20dp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/textView17"
        android:layout_toEndOf="@+id/textView17" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Outback Steakhouse"
        android:id="@+id/textView18"
        android:textColor="#ffa500"
        android:textSize="30dp"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Wendy&apos;s"
        android:id="@+id/textView36"
        android:layout_marginTop="22dp"
        android:textSize="30dp"
        android:textColor="#ffa500"
        android:layout_below="@+id/textView18"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Scarpetta NYC"
        android:id="@+id/textView37"
        android:layout_marginTop="24dp"
        android:layout_below="@+id/textView36"
        android:layout_centerHorizontal="true"
        android:textColor="#ffa500"
        android:textSize="30dp" />

</RelativeLayout>
